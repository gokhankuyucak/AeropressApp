[{
    "name": "Standard Method",
    "imgUrl": "assets/image/aeropress.png",
    "duration": "105",
    "info": {
      "coffee": "1 AeroPress Scoop",
      "water": "260g @85°C",
      "brewer": "Upright",
      "filter": "Paper"
    },
    "steps": [{
        "id": "1",
        "stepName": "Step 1",
        "hasTimer": "false",
        "description": "Put the desired amount of ground coffee into the chamber (two AeroPress scoops will make one double espresso, one Americano or one latte)."
      },
      {
        "id": "2",
        "stepName": "Step 2",
        "hasTimer": "true",
        "duration": "5",
        "description": "Pour the correlating amount of hot water (80ºC  for optimum flavour as anything higher scalds the beans), into the brewing chamber (up to 1 for 1 espresso etc. for a double espresso and for 1 Americano pour up to mark 2)."
      },
      {
        "id": "3",
        "name": "Add Water",
        "hasTimer": "true",
        "duration": "10",
        "description": "Mix the grinds and water with your stirring paddle for about 10 seconds to assist in pre-infusion."
      },
      {
        "id": "4",
        "name": "Stir",
        "hasTimer": "false",
        "description": "Now wet the rubber seal and insert the plunger into the chamber"
      },
      {
        "id": "5",
        "name": "Bloom",
        "hasTimer": "true",
        "duration": "30",
        "description": "Apply gentle pressure to the plunger, guiding it down towards the grinds. It should take about 20–30 seconds to reach the bottom."
      }
    ]
  },

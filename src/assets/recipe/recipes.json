    "info": {
      "coffee": "17.5g",
      "water": "270g Spa Blue mineral water @ 82°C / 76°C",
      "brewer": "Inverted",
      "filter": "Paper"
    },
    "steps": [{
        "id": "1",
        "name": "Coffee",
        "hasTimer": "false",
        "description": "Pick out all coffee beans that are pale, too big, too small or ear-shaped."
      },
      {
        "id": "2",
        "name": "Grind",
        "hasTimer": "false",
        "description": "Grind coffee at '8' on a Malkoenig EK43"
      },
      {
        "id": "3",
        "name": "Filter",
        "hasTimer": "false",
        "description": "Tip ground coffee into a Sowden Softbrew filter"
      },
      {
        "id": "4",
        "name": "Stir",
        "hasTimer": "false",
        "description": "Bang and shake the filter to get rid of the super-fines (You can also lose some of the chaff this way)"
      },
      {
        "id": "5",
        "name": "Water",
        "hasTimer": "false",
        "description": "Add around 40g of water at 82°C, being sure to wet all the grounds evenly"
      },
      {
        "id": "6",
        "name": "Bloom",
        "hasTimer": "true",
        "duration": "30",
        "description": "Allow to bloom for 30 seconds - shake lightly there are dry spots"
      },
      {
        "id": "7",
        "name": "Water",
        "hasTimer": "true",
        "duration": "60",
        "description": "Add 230g of water at 76°C in an extremely slow pour: it should take 60 seconds"
      },
      {
        "id": "8",
        "name": "Press",
        "hasTimer": "true",
        "duration": "30",
        "description": "Press down gently for 30 seconds"
      },
      {
        "id": "9",
        "name": "Leave",
        "hasTimer": "false",
        "description": "Leave about 50g of slurry in the AeroPress"
      }
    ]
  },
